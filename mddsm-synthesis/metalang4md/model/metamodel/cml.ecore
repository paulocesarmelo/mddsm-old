    <eStructuralFeatures xsi:type="ecore:EAttribute" name="voiceCommand" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="action" eType="#//ActionTypeForm"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="layoutSpec" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="BuiltInType">
    <eLiterals name="TEXT_FILE"/>
    <eLiterals name="LIVE_AUDIO" value="1"/>
    <eLiterals name="LIVE_VIDEO" value="2"/>
    <eLiterals name="LIVE_AV" value="3"/>
    <eLiterals name="TEXT" value="4"/>
    <eLiterals name="AUDIO_FILE" value="5"/>
    <eLiterals name="VIDEO_FILE" value="6"/>
    <eLiterals name="AV_FILE" value="7"/>
    <eLiterals name="NON_STREAM_FILE" value="8"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ActionTypeRequest">
    <eLiterals name="OPEN"/>
    <eLiterals name="SAVE" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ActionTypeForm">
    <eLiterals name="SEND"/>
    <eLiterals name="SEND_ON_DEMAND" value="1"/>
    <eLiterals name="SECURE_SEND" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlSchema" eSuperTypes="#//CommunicationSchema">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" upperBound="-1"
