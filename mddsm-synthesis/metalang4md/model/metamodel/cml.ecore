    <eStructuralFeatures xsi:type="ecore:EReference" name="attachedParty" lowerBound="1"
        upperBound="-1" eType="#//AttachedParty" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataType" lowerBound="1"
        upperBound="-1" eType="#//DataType" containment="true"/>
