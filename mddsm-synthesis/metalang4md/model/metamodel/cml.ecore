<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="cml" nsURI="http://www.inf.ufg.br/metalang4md/cml" nsPrefix="cml">
  <eClassifiers xsi:type="ecore:EClass" name="CommunicationSchema"/>
  <eClassifiers xsi:type="ecore:EClass" name="DataSchema" eSuperTypes="#//CommunicationSchema">
    <eStructuralFeatures xsi:type="ecore:EReference" name="request" eType="#//Request"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataContent" eType="#//DataContent"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="connectionID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Request">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="requestID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MediaRequest" eSuperTypes="#//Request">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mediumName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="requestAction" eType="#//ActionTypeRequest"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FormRequest" eSuperTypes="#//Request">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="formID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mediumURL" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataContent"/>
