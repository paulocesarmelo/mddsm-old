  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FormTypeRef" eSuperTypes="#//DataTypeRef">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="formTypeName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MediumTypeRef" eSuperTypes="#//DataTypeRef">
