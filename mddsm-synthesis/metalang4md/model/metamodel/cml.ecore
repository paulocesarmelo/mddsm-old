    <eStructuralFeatures xsi:type="ecore:EReference" name="device" lowerBound="1"
        upperBound="-1" eType="#//Device" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="connectionID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AttachedParty" eSuperTypes="../metalang4md.ecore#//EActor">
    <eStructuralFeatures xsi:type="ecore:EReference" name="isAttached" lowerBound="1"
        eType="#//IsAttached" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="person" lowerBound="1"
        eType="#//Person" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataType" eSuperTypes="../metalang4md.ecore#//EDomainSpecificType"/>
  <eClassifiers xsi:type="ecore:EClass" name="FormType" eSuperTypes="#//DataType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="subFormType" upperBound="-1"
        eType="#//FormType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subMediumType" upperBound="-1"
        eType="#//MediumType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="actionTF" eType="#//ActionTypeForm"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="suggestedApp" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
