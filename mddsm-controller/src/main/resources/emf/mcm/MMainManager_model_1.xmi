<?xml version="1.0" encoding="ASCII"?>
<sb.base:Manager
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sb.base="http://www.inf.ufg.br/mestrado/sb/base"
    xmlns:sb.base.common="http://www.inf.ufg.br/mestrado/sb/base/common"
    xsi:schemaLocation="http://www.inf.ufg.br/mestrado/sb/base base.ecore http://www.inf.ufg.br/mestrado/sb/base/common base.ecore#//common"
    name="MMainManager">
  <iface Name="MMainManagerInterface">
    <provides
        name="DispatchScript">
      <parameters
          name="script"/>
    </provides>
    <signals
        name="MiddlewareEvent"/>
  </iface>
  <actions xsi:type="sb.base.common:MacroAction"
      name="DispatchScript"
      impl="middleware.actions.mm.DispatchScript">
    <parameters
        name="aScript"/>
  </actions>
  <actions xsi:type="sb.base.common:MacroAction"
      name="MiddlewareEvent"
      impl="middleware.actions.mm.MiddlewareEvent"/>
  <handlers
      signal="//@iface/@provides.0">
    <action
        action="//@actions.0">
      <bindings
          parameter="//@actions.0/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.0/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@iface/@signals.0">
    <action
        action="//@actions.1"/>
  </handlers>
  <stateManager>
    <stateTypes
        name="MainManagerState"
        key="//@stateManager/@stateTypes.0/@properties.0">
      <properties
          name="id"/>
      <properties
          name="scriptManager"/>
      <properties
          name="scriptQueue"/>
    </stateTypes>
  </stateManager>
  <resourceManager
      xsi:type="sb.base:InstanceResourceManager">
    <iface/>
  </resourceManager>
  <policyManager/>
</sb.base:Manager>
