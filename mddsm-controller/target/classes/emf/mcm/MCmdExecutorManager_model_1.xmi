<?xml version="1.0" encoding="ASCII"?>
<sb.base:Manager
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:sb.base="http://www.inf.ufg.br/mestrado/sb/base"
    xmlns:sb.base.common="http://www.inf.ufg.br/mestrado/sb/base/common"
    xsi:schemaLocation="http://www.inf.ufg.br/mestrado/sb/base base.ecore http://www.inf.ufg.br/mestrado/sb/base/common base.ecore#//common"
    name="CommandExecutor">
  <iface>
    <provides
        name="RemoveDevice"/>
    <provides
        name="InitializeMGrid">
      <parameters
          name="mgridID"/>
    </provides>
    <provides
        name="AddLoadController">
      <parameters
          name="controllerID"/>
      <parameters
          name="name"/>
    </provides>
    <provides
        name="AddLoadDeviceType"/>
    <provides
        name="AddLoadDevice">
      <parameters
          name="deviceID"/>
    </provides>
    <provides
        name="SetDeviceProperty"/>
  </iface>
  <actions xsi:type="sb.base.common:MacroAction"
      name="RemoveDevice"
      impl="middleware.actions.ce.RemoveDevice"/>
  <actions xsi:type="sb.base.common:MacroAction"
      name="InitializeMGrid"
      impl="middleware.actions.ce.InitializeMGrid"/>
  <actions xsi:type="sb.base.common:MacroAction"
      name="AddLoadController"
      impl="middleware.actions.ce.AddLoadController"/>
  <actions xsi:type="sb.base.common:MacroAction"
      name="AddLoadDeviceType"
      impl="middleware.actions.ce.AddLoadDeviceType"/>
  <actions xsi:type="sb.base.common:MacroAction"
      name="AddLoadDevice"
      impl="middleware.actions.ce.AddLoadDevice"/>
  <actions xsi:type="sb.base.common:MacroAction"
      name="SetDeviceProperty"
      impl="middleware.actions.ce.SetDeviceProperty"/>
  <handlers
      signal="//@iface/@provides.0">
    <action
        action="//@actions.0"/>
  </handlers>
  <handlers
      signal="//@iface/@provides.1">
    <action
        action="//@actions.1">
      <bindings
          parameter="//@iface/@provides.1/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.1/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@iface/@provides.2">
    <action
        action="//@actions.2">
      <bindings
          parameter="//@iface/@provides.2/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.2/@parameters.0"/>
      </bindings>
      <bindings
          parameter="//@iface/@provides.2/@parameters.1">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.2/@parameters.1"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@iface/@provides.3">
    <action
        action="//@actions.3"/>
  </handlers>
  <handlers
      signal="//@iface/@provides.4">
    <action
        action="//@actions.4">
      <bindings
          parameter="//@iface/@provides.4/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.4/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <handlers
      signal="//@iface/@provides.5">
    <action
        action="//@actions.5">
      <bindings
          parameter="//@iface/@provides.4/@parameters.0">
        <value
            xsi:type="sb.base.common:ParameterValue"
            parameter="//@iface/@provides.4/@parameters.0"/>
      </bindings>
    </action>
  </handlers>
  <stateManager>
    <stateTypes
        name="CommandExecutorState"
        key="//@stateManager/@stateTypes.0/@properties.0">
      <properties
          name="id"/>
      <properties
          name="broker"/>
    </stateTypes>
  </stateManager>
  <resourceManager
      xsi:type="sb.base:InstanceResourceManager">
    <iface/>
  </resourceManager>
  <policyManager/>
</sb.base:Manager>
